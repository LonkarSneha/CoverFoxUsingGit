package encapsulationStusy;

public class UseofEncapsulation 
{
    private int a=2;
    private int b=4;
    
	public static void main(String[] args) 
	{
		UseofEncapsulation obj = new UseofEncapsulation();
		obj.display();
		obj.getvalue();
		obj.getvalueofb();
	}
	
	public void display()
	{
		int sub= b-a;
		System.out.println("The substraction is "+sub);
	}
	
	public int getvalue()
	{
		System.out.println("The value of a"+a);
	}
	
	public int getvalueofb()
	{
		return b;
	}

}
